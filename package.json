{
  "name": "devpulse",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "webhook": "ngrok http --url=deep-ready-foal.ngrok-free.app 3000",
    "api": "pnpm --filter api",
    "web": "pnpm --filter web",
    "db:push": "pnpm --filter db db:push",
    "db:studio": "pnpm --filter db db:studio",
    "gen": "turbo gen workspace",
    "sync:list-mis": "syncpack list-mismatches"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.4",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
